<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bupt.ctrl.model.Chapter" table="chapter" schema="sdrb">
        <id name="chapterId" column="Chapter_id"/>
        <property name="chapterNum" column="Chapter_Num"/>
        <property name="chapterName" column="Chapter_Name"/>
        <property name="chapterVideo" column="Chapter_Video"/>
        <property name="chapterPpt" column="Chapter_PPT"/>
        <property name="chapterIntro" column="Chapter_Intro"/>
    </class>
</hibernate-mapping>