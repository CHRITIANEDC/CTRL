<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bupt.ctrl.model.Course" table="course" schema="sdrb">
        <id name="courseId" column="Course_Id"/>
        <property name="courseName" column="Course_Name"/>
        <property name="coursePass" column="Course_Pass"/>
        <property name="courseIntro" column="Course_Intro"/>
        <property name="courseImage" column="Course_Image"/>
        <property name="courseDate" column="Course_Date"/>
        <property name="courseTeacher" column="Course_Teacher"/>
    </class>
</hibernate-mapping>