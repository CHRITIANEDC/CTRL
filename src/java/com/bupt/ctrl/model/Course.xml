<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.bupt.ctrl.model.Course">
        <table name="course" schema="sdrb" catalog=""/>
        <attributes>
            <id name="courseId">
                <column name="Course_Id"/>
            </id>
            <basic name="courseName">
                <column name="Course_Name"/>
            </basic>
            <basic name="coursePass">
                <column name="Course_Pass"/>
            </basic>
            <basic name="courseIntro">
                <column name="Course_Intro"/>
            </basic>
            <basic name="courseImage">
                <column name="Course_Image"/>
            </basic>
            <basic name="courseDate">
                <column name="Course_Date"/>
            </basic>
            <basic name="courseTeacher">
                <column name="Course_Teacher"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
