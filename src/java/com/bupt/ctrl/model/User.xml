<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.bupt.ctrl.model.User">
        <table name="user" schema="sdrb" catalog=""/>
        <attributes>
            <id name="userId">
                <column name="User_id"/>
            </id>
            <basic name="userName">
                <column name="User_Name"/>
            </basic>
            <basic name="userPassword">
                <column name="User_Password"/>
            </basic>
            <basic name="userIdentity">
                <column name="User_Identity"/>
            </basic>
            <basic name="userIntro">
                <column name="User_Intro"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
