<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.bupt.ctrl.model.UserHasChapter" table="user_has_chapter" schema="sdrb">
        <composite-id mapped="true" class="com.bupt.ctrl.model.UserHasChapterPK">
            <key-property name="userUserId" column="User_User_id"/>
            <key-property name="chapterChapterId" column="Chapter_Chapter_id"/>
        </composite-id>
        <property name="userHasLearned" column="User_has_Learned"/>
    </class>
</hibernate-mapping>